#include <iostream>
#include<bits/stdc++.h>
using namespace std;




int main() {
	// your code goes here
	string a;
	string b="hello";
	cin>>a;
	int j=0,count=0;
	for(int i=0;i<a.size();i++){
	    if(a[i]==b[j]){
	        j++;
	        count++;
	        
	         if(count==5)
	    break;
	    }
	   
	}
	if(count==5)
	    cout<<"YES"<<endl;
	    else
	    cout<<"NO"<<endl; 
	    
	    
	return 0;
}
